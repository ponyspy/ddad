.members_block
	margin-left -20px
	margin-right -20px
	font-family 'HelveticaNeue Roman'
	font-size 12pt
	line-height 14pt
	max-width 800px
	margin 0 auto
	.members_title
		padding-left 20px
		padding-right 20px
		margin-bottom 20px
		font-family 'HelveticaNeue Bold'
		font-size 18pt
		line-height 20pt
		text-transform uppercase
	.members_group
		margin-bottom 40px
	.member_item
		display inline-block
		vertical-align top
		width 25%
		margin-right -4px
		padding-left 20px
		padding-right 20px
		box-sizing border-box
		margin-bottom 20px
		.member_name
			font-family 'HelveticaNeue Bold'
			margin-bottom 5px
		.project_num
			margin-right 7px
			color black
			text-decoration none
			display inline-block
			&:after
				content ','
			&:last-child:after
				content ''