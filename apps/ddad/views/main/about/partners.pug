extends ../layout

block header_meta
	- var content_title= 'DD:A|D / about / partners'
	//- - var content_description= __('')
	- var about = 'partners'

block header_include
	- var include_css = ['about.css', 'partners.css']
	- var include_js = ['partners.js']

block content
	.partners_block
		each partner in partners
			a.partner_item(href=partner.link, target='_blank', class='size_' + partner.size)
				.partner_logo(style='background-image: url(' + partner.logo + ')')
				.partner_title= partner.i18n.title.get(locale)